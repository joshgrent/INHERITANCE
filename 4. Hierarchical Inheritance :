class EsportsEvent:
    def schedulematch(self):
        print("this esportsevent can schedulematch.")

class StudentDivision(EsportsEvent):
    def registerteam(self):
        print("this studentdivision can registerteam.")

class Facultydivision(EsportsEvent):
    def assignreferee(self):
        print("this facultydivision can assignreferee.")
        
Facultydivision = Facultydivision()
StudentDivision = StudentDivision()
Facultydivision.schedulematch()
Facultydivision.assignreferee()
StudentDivision.schedulematch()
StudentDivision.registerteam()
